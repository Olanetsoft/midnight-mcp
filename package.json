{
  "name": "midnight-mcp",
  "version": "0.1.34",
  "description": "Model Context Protocol Server for Midnight Blockchain Development",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "midnight-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "midnight",
    "blockchain",
    "mcp",
    "model-context-protocol",
    "compact",
    "zero-knowledge",
    "ai",
    "developer-tools"
  ],
  "author": "Idris Olubisi",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chromadb": "^1.8.1",
    "dotenv": "^16.3.1",
    "octokit": "^3.1.2",
    "openai": "^4.20.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/tar": "^6.1.13",
    "prettier": "^3.1.0",
    "tar": "^7.5.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Olanetsoft/midnight-mcp.git"
  },
  "homepage": "https://github.com/Olanetsoft/midnight-mcp#readme",
  "bugs": {
    "url": "https://github.com/Olanetsoft/midnight-mcp/issues"
  }
}
